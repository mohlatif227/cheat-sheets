{
  "name": "cheat-sheets",
  "version": "2.0.0",
  "author": "Scott Spence <spences10apps@gmail.com>(https://scottspence.me/)",
  "license": "MIT",
  "description": "Cheat sheets",
  "scripts": {
    "postinstall": "patch-package",
    "breadbox": "breadbox --dest public/web_modules",
    "build": "linaria -o public/styles src/**/*.js && toast incremental",
    "serve": "npx http-server public",
    "prepare": "breadbox --dest public/web_modules",
    "clean": "rm -rf .cache public && breadbox --dest public/web_modules",
    "format": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.16",
    "@sector/babel-plugin-package-import": "^0.3.1",
    "@sector/breadbox": "^0.0.4",
    "linaria": "^1.3.3",
    "patch-package": "^6.2.2",
    "preact": "^10.4.8",
    "preact-render-to-string": "^5.1.10",
    "react-helmet": "^6.1.0",
    "toast": "^0.2.10",
    "victormono": "^1.4.1"
  },
  "devDependencies": {
    "husky": "4.2.5",
    "prettier": "2.1.1",
    "pretty-quick": "3.0.0"
  },
  "snowpack": {
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "preact/compat",
      "react-helmet"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cheat-sheets.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/spences10/cheat-sheets/issues"
  },
  "homepage": "https://github.com/spences10/cheat-sheets#readme"
}
